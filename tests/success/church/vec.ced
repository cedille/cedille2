module church/vec.

import nat.

Vec : ★ ➔ Nat ➔ ★
= λ A. λ n. ∀ X:Nat ➔ ★. X zero ➔ (∀ n:Nat. A ➔ X n ➔ X (succ n)) ➔ X n.

vnil : ∀ A:★. Vec·A zero
= Λ A. Λ X. λ nil. λ cons. nil.

vcons : ∀ A:★. ∀ n:Nat. A ➔ Vec·A n ➔ Vec·A (succ n)
= Λ A. Λ n. λ a. λ l. Λ X. λ nil. λ cons. cons -n a (l·X nil cons).
